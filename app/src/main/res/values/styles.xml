<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Light.NoTitleBar">
    <!--<style name="AppTheme" parent="android:Theme.Light.NoTitleBar">-->
        <!-- Customize your theme here. -->
    </style>

    <style name="LiveActivityStyle">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowNoDisplay">true</item>
    </style>

    <style name="MainAtvItemLinearLayoutStyle">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/sp_1_tile_bg_normal</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="MainAtvItemLinearLayoutStyle2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">290px</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MainAtvItemViewFrameStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="et_login_pwd">
        <item name="android:background">@drawable/sel_0_login_ps_bg</item>
        <item name="android:paddingLeft">29px</item>
        <item name="android:layout_width">@dimen/login_row_width</item>
        <item name="android:layout_height">@dimen/login_row_heigt</item>
        <item name="android:drawablePadding">26px</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">#fdb790</item>
    </style>

    <style name="login_height_width">
        <item name="android:layout_width">@dimen/login_row_width</item>
        <item name="android:layout_height">@dimen/login_row_heigt</item>
    </style>

    <style name="settting_text_item">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/margin_normal_40px</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/font_normal_46px</item>
    </style>

    <style name="setting_img_item">
        <item name="android:layout_marginRight">@dimen/margin_normal_40px</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@drawable/right_arrow_setting</item>
    </style>

    <style name="setting_rl_item_1">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/sel_s_item_bg1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height_setting_150px</item>
    </style>

    <style name="setting_rl_item_2">
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/sel_s_item_bg2</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height_setting_150px</item>
    </style>

    <!--警情表单的一些样式-->
    <style name="fire_case_detail_tv_style1">
        <item name="android:layout_weight">2.2</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">13sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:background">@drawable/fire_case_table_bg</item>
    </style>
    <style name="fire_case_detail_tv_style2">
        <item name="android:layout_weight">3</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">13sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:background">@drawable/fire_case_table_bg</item>
    </style>
    <style name="fire_case_detail_tv_style3">
        <item name="android:layout_weight">8.2</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:background">@drawable/fire_case_table_bg</item>
    </style>
    <style name="fire_case_detail_tv_style4">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:background">@drawable/fire_case_table_bg</item>
    </style>
    <style name="fire_case_detail_tv_style5">
        <item name="android:layout_weight">6</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:background">@drawable/fire_case_table_bg</item>
    </style>

    <style name="fire_case_detail_ll_style1">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="fire_case_detail_ll_style2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">2</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <!--警情表单的一些样式-->
</resources>
